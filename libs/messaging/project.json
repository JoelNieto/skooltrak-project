{
  "projectType": "library",
  "root": "libs/messaging",
  "sourceRoot": "libs/messaging/src",
  "prefix": "skooltrak-project",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/messaging"],
      "options": {
        "jestConfig": "libs/messaging/jest.config.js",
        "passWithNoTests": true
