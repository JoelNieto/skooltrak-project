    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/messaging/src/**/*.ts",
          "libs/messaging/src/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
